<%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
        <div class="row pt5 pb5">
          <div class="col-md-12">
            <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: "btn btn-md btn-primary col-md-12"%><br />
          </div>
        </div>
        <div class="row pt5 pb5">
          <div class="col-md-12">
            <div class="text-center">
              OR
            </div>
          </div>
        </div>
    <% end -%>
<% end -%>