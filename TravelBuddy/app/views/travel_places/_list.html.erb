<table class="table table-bordered">
  <thead>
    <tr>
      <th>Address</th>
      <th>Starttime</th>
      <th>Endtime</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% travel_places.each do |travel_place| %>
      <tr>
        <td><%= travel_place.address %></td>
        <td><%= travel_place.startTime %></td>
        <td><%= travel_place.endTime %></td>
        <td><%= travel_place.description %></td>
        <td><%= link_to 'Show', travel_event_travel_place_path(id: travel_place, travel_event_id: travel_place.travel_event.id) %></td>
        <td><%= link_to 'Edit', edit_travel_event_travel_place_path(id: travel_place, travel_event_id: travel_place.travel_event.id) %></td>
        <td><%= link_to 'Destroy', travel_event_travel_place_path(id: travel_place, travel_event_id: travel_place.travel_event.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

  <tfoot>
    <tr>
    <th colspan="7" class="text-center">
      <%= link_to 'Add new', new_travel_event_travel_place_url(travel_event_id: @travel_event.id) %>
    </th>
    </tr>
  </tfoot>
</table>

<br>